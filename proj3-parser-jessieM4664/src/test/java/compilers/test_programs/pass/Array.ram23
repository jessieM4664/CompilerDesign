class Parse2 {
  public static void main (String[] a) {
    print(new Sum().age(23, 47, 38, 0));
  }
}

class Sum {
  public integer age(integer i, integer j, integer k, integer sum) {
    integer[] ages;

    ages[0] = i;
    ages[1] = j;
    ages[2] = k;

    while (ages.length)  {
      sum += ages[i];
    }
    return sum;
  }
}